<Window x:Class="PackageEasy.Views.Dialogs.Wating"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PackageEasy.Views.Dialogs"
        xmlns:controls="https://dicgo.com/controls"
        mc:Ignorable="d"
        Title="Wating" Height="450" Background="Transparent" ShowInTaskbar="False" Width="800" WindowStyle="None" WindowStartupLocation="CenterOwner" AllowsTransparency="True">
    <Grid Background="Transparent">
        <Border Background="{DynamicResource watingBackground}" Opacity="0.5"></Border>
        <controls:TProgressRing Style="{StaticResource CircleProgressRingStyle}" Width="80" Height="80"  IsActive="True"></controls:TProgressRing>
    </Grid>
</Window>
